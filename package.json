{
  "name": "ballsville-game",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "next dev",
    "proxy": "npx wrangler pages dev --proxy 3000 --port 8788 --persist-to .wrangler/state",
    "seed": "npx wrangler r2 object put admin/data/gauntlet/leagues_2025.json --file r2/data/gauntlet/leagues_2025.json --local --persist-to .wrangler/state",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "icons": "node scripts/make-icons.mjs public/logo-source.webp",
    "git": "git add . && git commit -m \"main_constitution\" && git push origin preview",
    "leg3": "node script/buildgauntlet.mjs",
    "leaderboards": "node script/leaderboards/auto-gen.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.946.0",
    "@supabase/supabase-js": "^2.78.0",
    "axios": "^1.13.2",
    "dotenv": "^17.2.3",
    "framer-motion": "^12.23.24",
    "next": "^16.0.1",
    "p-limit": "^7.2.0",
    "prompts": "^2.4.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "5.3.0"
  },
  "devDependencies": {
    "autoprefixer": "10.4.21",
    "baseline-browser-mapping": "^2.8.32",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.5",
    "postcss": "8.5.6",
    "sharp": "^0.34.4",
    "tailwindcss": "3.4.13",
    "to-ico": "^1.0.1",
    "wrangler": "^4.56.0"
  },
  "browserslist": [
    "defaults and not ie 11 and not op_mini all"
  ]
